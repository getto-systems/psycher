  "license": "MIT",
  "devDependencies": {
    "exec-on-changed": "^1.2.0",
    "jest": "^26.4.0"
  },
  "dependencies": {
    "getto-aws_dynamodb": "^1.3.4",
    "getto-aws_secrets": "^1.0.3",
    "getto-gitlab_api": "^1.0.4",
    "getto-memoize": "^1.0.6",
    "getto-slack_api": "^1.0.5",
    "getto-specification": "^1.0.3",
    "uuid": "^8.3.1"
  }
}
